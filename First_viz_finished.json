
{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "params": [
    {
      "name": "visualization",
      "value": "education",
      "bind": {
        "input": "select",
        "options": ["education", "employment"],
        "labels": ["Education Costs", "Employment Rate"],
        "name": "Select Visualization: "
      }
    }
  ],
  "width": 700,
  "height": 520,
  "projection": {"type": "equirectangular"},
  "title": {
    "text": {"expr": "visualization == 'education' ? 'Australia: Median Estimated Cost for International Students' : 'Employment-to-Population Rate by State'"},
    "subtitle": {"expr": "visualization == 'education' ? 'Choropleth by state (Median of Total_Est_AUD per state)' : 'Colour = Employment-to-pop (%) â€¢ Tooltip also shows Unemployment (%)'"}
  },
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/hoanggiaphat-monash/FIT3179-Assignment-2/refs/heads/main/STE_2021_AUST.topojson",
        "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
      },
      "mark": {"type": "geoshape", "fill": "#e5e5e5", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/hoanggiaphat-monash/FIT3179-Assignment-2/refs/heads/main/clean_state_labour_force.csv",
        "format": {
          "type": "csv",
          "parse": {
            "Employment_to_pop_pct": "number",
            "Unemployment_rate_pct": "number"
          }
        }
      },
      "transform": [
        {
          "lookup": "State",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/hoanggiaphat-monash/FIT3179-Assignment-2/refs/heads/main/STE_2021_AUST.topojson",
              "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
            },
            "key": "properties.STE_NAME21"
          },
          "as": "geo"
        },
        {"filter": "visualization == 'employment'"}
      ],
      "mark": {"type": "geoshape", "stroke": "white", "strokeWidth": 1},
      "encoding": {
        "shape": {"field": "geo", "type": "geojson"},
        "color": {
          "field": "Employment_to_pop_pct",
          "type": "quantitative",
          "title": "Employment-to-pop (%)",
          "scale": {"scheme": "greens"},
          "legend": {"format": ".1f"}
        },
        "tooltip": [
          {"field": "State", "type": "nominal", "title": "State"},
          {"field": "Employment_to_pop_pct", "type": "quantitative", "title": "Employment-to-pop (%)", "format": ".1f"},
          {"field": "Unemployment_rate_pct", "type": "quantitative", "title": "Unemployment (%)", "format": ".1f"}
        ]
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/hoanggiaphat-monash/FIT3179-Assignment-2/refs/heads/main/International_Education_Costs.csv",
        "format": {
          "type": "csv",
          "parse": {"Total_Est_AUD": "number"}
        }
      },
      "transform": [
        {
          "calculate": "datum.State=='NSW' ? 'New South Wales' : datum.State=='VIC' ? 'Victoria' : datum.State=='QLD' ? 'Queensland' : datum.State=='SA' ? 'South Australia' : datum.State=='WA' ? 'Western Australia' : datum.State=='TAS' ? 'Tasmania' : datum.State=='NT' ? 'Northern Territory' : datum.State=='ACT' ? 'Australian Capital Territory' : datum.State",
          "as": "State_Full_Name"
        },
        {
          "aggregate": [
            {"op": "median", "field": "Total_Est_AUD", "as": "MedianCost_AUD"},
            {"op": "count", "as": "UniCount"}
          ],
          "groupby": ["State_Full_Name"]
        },
        {
          "lookup": "State_Full_Name",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/hoanggiaphat-monash/FIT3179-Assignment-2/refs/heads/main/STE_2021_AUST.topojson",
              "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
            },
            "key": "properties.STE_NAME21"
          },
          "as": "geo"
        },
        {"filter": "visualization == 'education'"}
      ],
      "mark": {"type": "geoshape", "stroke": "white", "strokeWidth": 1},
      "encoding": {
        "shape": {"field": "geo", "type": "geojson"},
        "color": {
          "field": "MedianCost_AUD",
          "type": "quantitative",
          "title": "Median Cost per Year (AUD)",
          "scale": {"scheme": "Greens"},
          "legend": {"format": ",.0f"}
        },
        "tooltip": [
          {"field": "State_Full_Name", "type": "nominal", "title": "State"},
          {"field": "MedianCost_AUD", "type": "quantitative", "title": "Median cost (AUD)", "format": ",.0f"},
          {"field": "UniCount", "type": "quantitative", "title": "Universities counted"}
        ]
      }
    }
  ],
  "config": {"view": {"stroke": null}, "font": "Arial"}
}
